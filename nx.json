{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": ["{projectRoot}/src/**/*"],
    "build": ["default", "{projectRoot}/tsconfig.json", "{projectRoot}/package.json"]
  },
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["build", "^build"],
      "outputs": ["{projectRoot}/dist"]
    }
  },
  "defaultBase": "main"
}
